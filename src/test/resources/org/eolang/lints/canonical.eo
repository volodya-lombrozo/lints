+package canonical
+version 0.0.0
+unlint unsorted-metas

# Times table, which is a canonical example of EO program.
[args] > canonical
  malloc.for > @
    0
    [x] >>
      seq > @
        *
          x.put 2
          while
            x.as-number.lt 6 > [i] >>
            [i] >>
              seq > @
                *
                  QQ.io.stdout
                    QQ.txt.sprintf
                      "%d x %d = %d\n"
                      *
                        ^.x
                        ^.x
                        ^.x.as-number.times ^.x
                  ^.x.put
                    ^.x.as-number.plus 1
          true
